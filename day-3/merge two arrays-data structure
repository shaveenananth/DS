#include<stdio.h>
int main()
{
    int a[100]={};
    int b[100]={};
    int c[100]={};
    int na,nb,nc;
    printf("\nEnter number of elements in array a");
    scanf("%d",&na);
    printf("\nEnter the elements of a");
    for(int i=0;i<na;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("\nEnter number of elements in array b");
    scanf("%d",&nb);
    printf("\nEnter the elements of b");
    for(int i=0;i<nb;i++)
    {
        scanf("%d",&b[i]);
    }
    nc=na+nb;
    for(int i=0;i<na;i++)
    {
        c[i]=a[i];    
    }
    for(int j=0;j<nb;j++)
    {
        c[na+j]=b[j];
    }
    printf("\nPrinting the merged array");
    for(int k=0;k<nc;k++)
    {
        printf("\n %d",c[k]);
    }
    
    
}
